{
    "name": "blendbyte/nova-items-field",
    "description": "Nova field to handle array columns",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NovaItemsField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaItemsField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
